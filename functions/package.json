{
  "name": "potlucks4change",
  "description": "A set of scripts for managing relational data for Potlucks for Change",
  "scripts": {
    "lint": "eslint .",
    "lintfix": "eslint --fix .",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "jest --verbose --watch",
    "syncconfig": "node syncconfig.js"
  },
  "engines": {
    "node": "20"
  },
  "main": "index.js",
  "dependencies": {
    "@notionhq/client": "^2.2.14",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.3.1",
    "google-auth-library": "^9.13.0",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^3.1.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  },
  "private": true
}
